
<center>
<form action="/faves"method="GET">
<h1>Favorite Pokemon</h1>
<a href="/">Home</a>
</center>

<ul class="list-group">
    <% allPokemon.forEach(favorite => { %>
      <form action="/" method="GET">
      <a href="/pokemon/<%= favorite.name.toLowerCase()%> ">
      <p class='favpoke'><%= favorite.name.toUpperCase()%></p>
    </a>
    <%  }) %>
    </ul>
</form>